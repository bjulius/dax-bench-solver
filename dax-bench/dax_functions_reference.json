  "deprecated_functions": {
    "EARLIER": {
      "status": "Not recommended",
      "reason": "Modern DAX best practice is to use VAR to capture values before nested row context",
      "replacement": "VAR pattern",
      "example_before": "COUNTROWS(FILTER(T, T[Col] <= EARLIER(T[Col])))",
      "example_after": "VAR CurrentVal = T[Col] RETURN COUNTROWS(FILTER(T, T[Col] <= CurrentVal))"
    },
    "EARLIEST": {
      "status": "Not recommended",
      "reason": "Same as EARLIER - use VAR pattern instead",
      "replacement": "VAR pattern"
    }
